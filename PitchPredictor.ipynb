    "def LogisticRegression(df, sample_fraction=0.7, input_cols=input_cols):\n",
    "    \n",
    "    df = df.dropna()\n",
    "    small_df = df.sample(10000)\n",
    "    \n",
    "    train, test = train_test_split(small_df, test_size=0.2)\n",
    "    \n",
    "    X_train = train[input_cols[0:len(input_cols) - 1]]\n",
    "    y_train = train[input_cols[-1]]\n",
    "    \n",
    "    X_test = test[input_cols[0:len(input_cols) - 1]]\n",
    "    y_test = test[input_cols[-1]]\n",
    "    \n",
    "    LGModel = LogisticRegression(n_estimators=100, max_depth=10, random_state=0)\n",
    "    LGModel.fit(X_train, y_train)\n",
    "    \n",
    "    predictions = LGModel.predict(X_train)\n",
    "    train['predictions'] = predictions\n",
    "    \n",
    "    predictions_test = LGModel.predict(X_test)\n",
    "    test['predictions'] = predictions_test\n",
    "    \n",
    "    score_train = LGModel.score(X_train, y_train)\n",
    "    score_test = LGModel.score(X_test, y_test)\n",
    "    \n",
    "    print(\"Training accuracy: \" + str(score_train))\n",
    "    print(\"Testing accuracy: \" + str(score_test))\n",
    "    \n",
    "    return train, test, LGModel"
   ]
