    "# This function encodes several columns including `batter side` and `pitcher throwing hand`\n",
    "def get_columns(df):\n",
    "    df['TM_DATE_TIME'] = pd.to_datetime(df['TM_DATE'] + ' ' + df['TM_TIME'])\n",
    "    df['BATTERSIDE'] = df['BATTERSIDE'].map({'Right': 1, 'Left': 0})\n",
    "    df['PITCHERTHROWS'] = df['PITCHERTHROWS'].map({'Right': 1, 'Left': 0})\n",
    "    return df[cols]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# This function normalizes each numerical column\n",
    "def normalize_cols(df):\n",
    "    df_copy = df.copy()\n",
    "    df_copy[cols_norm] = df_copy[cols_norm].apply(lambda x: (x - x.min()) / (x.max() - x.min()))\n",
    "    return df_copy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# This function encodes the label (pitch type) of the current pitch\n",
    "def pitch_labels(df):\n",
    "    df[pitch_type_col] = df[pitch_type_col].astype('category')\n",
    "    df[pitch_label_col] = df[pitch_type_col].cat.codes\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# This function encodes the outcome of the current pitch.\n",
    "def encoded(df):\n",
    "    pitchcall_col = 'PITCHCALL'\n",
    "    pitchcall_encoded_col = 'PitchOutcome'\n",
    "    df[pitchcall_col] = df[pitchcall_col].astype('category')\n",
    "    df[pitchcall_encoded_col] = df[pitchcall_col].cat.codes\n",
    "    \n",
    "    pitcher_col = 'PITCHER'\n",
    "    pitcher_encoded_col = 'Pitcher Id Norm'\n",
    "    df[pitcher_col] = df[pitcher_col].astype('category')\n",
    "    df[pitcher_encoded_col] = df[pitcher_col].cat.codes\n",
    "    \n",
    "    batter_col = 'BATTER'\n",
    "    batter_encoded_col = 'Batter Id Norm'\n",
    "    df[batter_col] = df[batter_col].astype('category')\n",
    "    df[batter_encoded_col] = df[batter_col].cat.codes\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Adds a few columns for the purposes of predicting the pitch given information about the following pitch \n",
    "# and the upcoming situation.\n",
    "# Columns include:\n",
    "# 1. Next batter\n",
    "# 2. Next pitch count (Balls, Strikes)\n",
    "# 3. Next outs\n",
    "# 4. Next inning\n",
    "\n",
    "def get_next_cols(df):\n",
    "    new_cols = {'BALLS': 'NEXT BALLS', 'STRIKES': 'NEXT STRIKES', \n",
    "                'BATTER': 'NEXT BATTER', 'Batter Id Norm': 'NEXT BATTERID',\n",
    "                'OUTS': 'NEXT OUTS', 'INNING': 'NEXT INNING', 'PAOFINNING': 'PA NEXT',\n",
    "                'Pitcher Id Norm': 'NEXT PITCHERID'}\n",
    "    \n",
    "    for col in new_cols:\n",
    "        \n",
    "        df[new_cols[col]] = df.groupby(['GAMEID', 'TOP_BOTTOM'])[col].shift(-1)\n",
    "        \n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def get_next_pitch_label(df, pitch_label_col='Next Pitch Label'):\n",
    "    df[pitch_label_col] = df.groupby(['GAMEID', 'TOP_BOTTOM'])['Pitch Type'].shift(-1)\n",
    "    return df "
   ]
  },
